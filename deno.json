{
	"name": "BoH-Helper",
	"exports": "./src/terminal.ts",
	"tasks": {
		"clean": "rm -r ./dist",
		"run": "deno run --allow-write=\"./config.json,./history.txt\" --allow-read --allow-sys=\"homedir\" --allow-env=\"TERM,COLORTERM,TERM_PROGRAM\" --deny-net --deny-env=\"SSH_CONNECTION\" ./src/terminal.ts",
		"compile": "deno compile --allow-write=\"./config.json,./history.txt\" --allow-read --allow-sys=\"homedir\" --allow-env=\"TERM,COLORTERM,TERM_PROGRAM\" --deny-net --deny-env=\"SSH_CONNECTION\" --output ./dist/BoH-Helper.exe ./src/terminal.ts && cp -r ./resources ./dist/resources",
	}
}